/* What does the app's SQL look like? */


CREATE TABLE tiktok_settings (
     id INTEGER PRIMARY KEY AUTOINCREMENT,
     username TEXT,
     fullname TEXT,
     password TEXT, 
     email TEXT,
     followers INTEGER,
     following INTEGER
     );
     
